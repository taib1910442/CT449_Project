{"ast":null,"code":"import axios from \"axios\";\nimport swal from \"sweetalert2\";\nexport default {\n  data() {\n    return {\n      \"isLoading\": false\n    };\n  },\n\n  methods: {\n    doRegister: async function () {\n      const form = event.target;\n      const formData = new FormData(form);\n      this.isLoading = true;\n      const response = await axios.post(this.$apiURL + \"/registration\", formData);\n      this.isLoading = false;\n      swal.fire(\"Success\", response.data.message, \"success\");\n\n      if (response.data.status == \"success\") {\n        this.$router.push({\n          path: \"/login\"\n        });\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAsCI,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,IAAP,MAAiB,aAAjB;AAEA,eAAe;AACXC,MAAI,GAAG;AACH,WAAO;AACH,mBAAa;AADV,KAAP;AAGH,GALU;;AAOXC,SAAO,EAAE;AACLC,cAAU,EAAE,kBAAkB;AAC1B,YAAMC,IAAG,GAAIC,KAAK,CAACC,MAAnB;AACA,YAAMC,QAAO,GAAI,IAAIC,QAAJ,CAAaJ,IAAb,CAAjB;AAEA,WAAKK,SAAL,GAAiB,IAAjB;AAEA,YAAMC,QAAO,GAAI,MAAMX,KAAK,CAACY,IAAN,CACnB,KAAKC,OAAL,GAAe,eADI,EAEnBL,QAFmB,CAAvB;AAKA,WAAKE,SAAL,GAAiB,KAAjB;AACAT,UAAI,CAACa,IAAL,CAAU,SAAV,EAAqBH,QAAQ,CAACT,IAAT,CAAca,OAAnC,EAA4C,SAA5C;;AAEA,UAAIJ,QAAQ,CAACT,IAAT,CAAcc,MAAd,IAAwB,SAA5B,EAAuC;AACnC,aAAKC,OAAL,CAAaC,IAAb,CAAkB;AACdC,cAAI,EAAE;AADQ,SAAlB;AAGJ;AACJ;AApBK;AAPE,CAAf","names":["axios","swal","data","methods","doRegister","form","event","target","formData","FormData","isLoading","response","post","$apiURL","fire","message","status","$router","push","path"],"sourceRoot":"","sources":["D:\\Download\\Compressed\\chat\\web\\src\\components\\RegisterComponent.vue"],"sourcesContent":["<template>\n    <div class=\"container\" style=\"margin-top: 50px;\">\n\n        <div class=\"row\">\n            <div class=\"col-md-12 text-center\">\n                <h1>Register</h1>\n            </div>\n        </div>\n\n        <div class=\"row\">\n            <div class=\"offset-md-3 col-md-6\">\n                <form method=\"POST\" v-on:submit.prevent=\"doRegister\">\n                    <div class=\"form-group\">\n                        <label>Enter name</label>\n                        <input type=\"text\" class=\"form-control\" name=\"name\" required />\n                    </div>\n                    <br />\n                    <div class=\"form-group\">\n                        <label>Enter email</label>\n                        <input type=\"email\" class=\"form-control\" name=\"email\" required />\n                    </div>\n                    <br />\n                    <div class=\"form-group\">\n                        <label>Enter password</label>\n                        <input type=\"password\" class=\"form-control\" name=\"password\" required />\n                    </div>\n\n                    <br />\n\n                    <input type=\"submit\" v-bind:value=\"isLoading ? 'Loading...' : 'Register'\" v-bind:disabled=\"isLoading\" name=\"submit\" class=\"btn btn-primary\" />\n                </form>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n\n    import axios from \"axios\"\n    import swal from \"sweetalert2\"\n\n    export default {\n        data() {\n            return {\n                \"isLoading\": false\n            }\n        },\n\n        methods: {\n            doRegister: async function () {\n                const form = event.target;\n                const formData = new FormData(form);\n\n                this.isLoading = true;\n\n                const response = await axios.post(\n                    this.$apiURL + \"/registration\",\n                    formData\n                );\n                \n                this.isLoading = false;\n                swal.fire(\"Success\", response.data.message, \"success\");\n\n                if (response.data.status == \"success\") {\n                    this.$router.push({\n                        path: \"/login\"\n                    })\n                }\n            }\n        }\n    }\n</script>"]},"metadata":{},"sourceType":"module"}